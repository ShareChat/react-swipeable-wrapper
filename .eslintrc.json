{
	"extends": ["airbnb", "prettier"],
	"plugins": ["react-hooks"],
	"parserOptions": {
		"ecmaVersion": 2020
	},
	"env": {
		"browser": true
	},
	"rules": {
		"import/no-extraneous-dependencies": "off",
		"no-param-reassign": "off",
		"react/jsx-props-no-spreading": [
			"error",
			{
				"html": "ignore"
			}
		],
		"no-nested-ternary": "off",
		"react/no-array-index-key": "off"
	}
}
